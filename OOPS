class student{
	   int rno;
	   String name;
	   float marks;
	   //we need a way to add the value above properties object
	   //by object
	   // We need one word to access every object
	   void greeting() {
		   System.out.print(this.name);
	   }
	   student(int rno,String name,float marks){
		   this.rno=rno;
		   this.marks=marks;
		   this.name=name;
	   }

}
